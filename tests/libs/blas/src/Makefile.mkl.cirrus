MF      = Makefile.libsci

SRC	= dgemv_benchmark.f90


FC      = mpif90
PP      = 

LDFLAGS =  -lmkl
FFLAGS  =  -O3 $(PP)
LIBS    = 

EXE	= dgemv_mkl.x

.SUFFIXES: .f90 .o

OBJ	=$(SRC:.f90=.o)

.f90.o:
	$(FC) -c $(FFLAGS) $<

all:	$(EXE)

$(EXE):$(OBJ)
	$(FC) $(LDFLAGS) -o $@ $(OBJ) $(LIBS)

$(OBJ): $(MF)

clean:
	rm -fr $(TMP) $(OBJ) $(EXE) core


