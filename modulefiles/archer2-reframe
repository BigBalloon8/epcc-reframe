#%Module##############################
# Modulefile for EPCC Reframe configuration
#

set is_module_rm [module-info mode remove]

module load reframe

set EPCC_REFRAME_DIR /work/y07/shared/utils/epcc-reframe

prepend-path PATH $EPCC_REFRAME_DIR/bin
setenv EPCC_REFRAME_CONFIG $EPCC_REFRAME_DIR/configuration/archer2_settings.py
setenv EPCC_REFRAME_TEST_DIR $EPCC_REFRAME_DIR/tests
